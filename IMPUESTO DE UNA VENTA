#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;


void main()
{	
	float impuesto,venta;
	cout<<"Ingrese la venta: ";
	cin>>venta;
	impuesto=venta*0.13;
	cout<<"El impuesto de la venta es= "<<impuesto;
	getch();
}
